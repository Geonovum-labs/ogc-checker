{
  "name": "@geonovum/ogc-checker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc -b && npm run build:rulesets && vite build",
    "build:rulesets": "tsdown src/index.ts --format esm --platform node --sourcemap --out-dir dist --clean --tsconfig tsconfig.rulesets.json --dts",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@stoplight/spectral-core": "^1.20.0",
    "@stoplight/spectral-functions": "^1.10.1",
    "@stoplight/spectral-rulesets": "^1.22.0",
    "luxon": "^3.7.2",
    "ramda": "^0.32.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "zustand": "^5.0.8",
    "@geonovum/standards-checker": "github:Geonovum/standards-checker"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/luxon": "^3.7.1",
    "@types/ramda": "^0.31.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  }
}
